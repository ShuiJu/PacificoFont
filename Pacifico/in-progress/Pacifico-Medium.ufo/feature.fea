# GSUB 


lookup midwordDLIGs {
  lookupflag 0;
    sub \i \x  by \i_x;
  subtable;
    sub \o \o \s  by \o_o_s;
  subtable;
    sub \r \s  by \r_s;
} midwordDLIGs;

lookup wordsubfor {
  lookupflag 0;
    sub \f \o \r  by \for;
} wordsubfor;

lookup subforNo. {
  lookupflag 0;
    sub \N \o \period  by \No;
} subforNo.;

lookup swashini {
  lookupflag 0;
    sub \f by \f.init ;
    sub \p by \p.init ;
    sub \s by \s.init ;
    sub \t by \t.init ;
} swashini;

