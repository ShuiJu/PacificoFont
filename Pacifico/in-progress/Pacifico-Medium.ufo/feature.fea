    sub [\space ] [\f ]'lookup swashini  ;
  subtable;
    sub [\space ] [\s ]'lookup swashini  ;
  subtable;
    sub [\space ] [\t ]'lookup swashini  ;
  subtable;
    sub [\N ]'lookup subforNo.  [\o ] [\period ] [\one \two \three \four \five \six \seven \eight \nine ] ;
  subtable;
